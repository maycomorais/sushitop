{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "eqeqeq": "error",
    "curly": "error",
    "no-multiple-empty-lines": ["error", { "max": 1, "maxEOF": 0 }]
  },
  "globals": {
    "supa": "readonly",
    "MENU": "readonly",
    "carrinho": "writable",
    "FONE_LOJA": "readonly"
  }
}